[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=www
Group=www-data
WorkingDirectory=/home/oleg/PycharmProjects/All_Project/telegr_django/src
ExecStart=/home/oleg/PycharmProjects/All_Project/telegr_django/env/bin/gunicorn --workers 3 --bind unix:/home/oleg/PycharmProjects/All_Project/telegr_django/gunicorn/gunicorn.sock config.wsgi:application --access-logfile /home/oleg/PycharmProjects/All_Project/telegr_django/gunicorn/access.log --error-logfile /home/oleg/PycharmProjects/All_Project/telegr_django/gunicorn/error.log
Restart=on-failure

[Install]
WantedBy=multi-user.target
